    <id>http://example.com/2022/04/19/CSC%E7%AC%AC%E4%BA%8C%E6%AC%A1%E4%BD%9C%E4%B8%9A%E5%8F%8D%E6%80%9D/</id>
    <published>2022-04-19T10:13:27.000Z</published>
